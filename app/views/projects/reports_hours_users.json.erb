{
	"name": "<%= @project.name %>",
	"children": [
		<% @project.users.each_with_index do |user, uindex| %>
		{
		"name": "<%= user.email %>",
		"children": [
			<% user.get_tasks_with_hours.each_with_index do |task_hour, index| %>
			{"name": "<%= task_hour.name %>", "size": <%= task_hour.size %>}<%= index < user.get_tasks_with_hours.count - 1 ? "," : "" %>
			<% end %>
		]
		}<%= uindex < @project.users.count - 1 ? "," : "" %>
		<% end %>
	]
}